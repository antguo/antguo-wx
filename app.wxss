/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.app__header-bar{
  width: 100%;
  height: 150rpx;
  text-align: center;
  line-height: 200rpx;
  color: white;
  font-size: 14px;
  background: linear-gradient(to right, #6960ef, #597df4);
}